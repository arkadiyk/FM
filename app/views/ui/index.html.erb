
<% if @api == 'real' %>
    <script type="text/javascript" src="https://apis.google.com/js/client.js"></script>
    <script type="text/javascript">
      window.GLOCSCRIPT = "https://www.google.com/jsapi"
    </script>
<% else %>
    <%= javascript_include_tag 'gapi-mock' %>
    <script type="text/javascript">
        setTimeout(function(){ window.gapi = (new GapiMock.GapiApi(window.FotomooFixtures)).getAPI() }, 1000)
        window.GLOCSCRIPT = "/assets/google-maps-api-mock.js"
    </script>
<% end %>